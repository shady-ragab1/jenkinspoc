node{


  stage('checkout'){
    git branch: 'master',
	credentialsId: 'githubCredentials',
	url: 'https://github.com/shady-ragab1/jenkinspoc'
  }

  stage('build jar'){

      //sh "${mvn} clean package"
	bat "mvn clean package"
  }

  stage('build image'){

        //sh "${mvn} clean package"
  	bat "mvn spring-boot:build-image"
    }





}
